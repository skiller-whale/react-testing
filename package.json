{
  "name": "react-testing-curriculum",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npx tsx server/server.tsx",
    "test:introduction:js": "vitest ./src_js/introduction",
    "test:introduction:ts": "vitest ./src_ts/introduction",
    "test:interactivity:js": "vitest ./src_js/interactivity",
    "test:interactivity:ts": "vitest ./src_ts/interactivity",
    "test:asynchronous_tests:js": "vitest ./src_js/asynchronous_tests",
    "test:asynchronous_tests:ts": "vitest ./src_ts/asynchronous_tests",
    "test:mocking:js": "vitest ./src_js/mocking",
    "test:mocking:ts": "vitest ./src_ts/mocking",
    "test:end_to_end_tests:js": "vitest ./src_js/end_to_end_tests",
    "test:end_to_end_tests:ts": "vitest ./src_ts/end_to_end_tests",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "compile": "tsc",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^24.1.0",
    "msw": "^2.3.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.9.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
